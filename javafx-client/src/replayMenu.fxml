<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<GridPane hgap="5.0" fx:controller="ReplayMenuController" xmlns:fx="http://javafx.com/fxml/1"
          xmlns="http://javafx.com/javafx/8">
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
        <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <Button fx:id="prevButton" onAction="#prevButtonAction" maxHeight="1.7976931348623157E308"
            maxWidth="1.7976931348623157E308">
        Prev
    </Button>
    <Button fx:id="nextButton" onAction="#nextButtonAction" maxHeight="1.7976931348623157E308"
            maxWidth="1.7976931348623157E308" GridPane.columnIndex="1">
        Next
    </Button>
    <Button fx:id="endButton" onAction="#endButtonAction"
            maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            GridPane.columnIndex="2">
        End Replay
    </Button>
</GridPane>